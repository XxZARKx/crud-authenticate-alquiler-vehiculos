{
	"builds": [
		{
			"src": "api/main.py",
			"use": "@vercel/python",
			"config": {
				"maxLambdaSize": "15mb",
				"runtime": "python3.11"
			}
		},
		{
			"src": "static/*",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/static/(.*)",
			"dest": "/static/$1"
		},
		{
			"src": "/(.*)",
			"dest": "api/main.py"
		}
	]
}
